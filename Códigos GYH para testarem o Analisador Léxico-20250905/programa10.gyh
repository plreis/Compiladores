#Não possui erro Léxico
[DECLARAR]
num1:INTEGER
num2:INTEGER
num3:INTEGER
aux:INTEGER

[PROGRAMA]
# Coloca 3 números em ordem crescente
LER num1
LER num2
LER num3
SE num1 > num2 ENTAO
   INI
      aux := 2.4+3-4+5-6*5-1.6
      num2 := num1
      num1 := aux
   FIM 
SE num1 > num3 E num2 <= num4 E num1 > 3 OU num2 != num4 ENTAO
   INI
      aux := num3
      num3 := num1
      num1 := aux
   FIM
SE num2 > num3 ENTAO
   INI
      aux:= num3
      num3:= num2
     num2:=aux
   FIM
IMPRIMIR num1
IMPRIMIR num2
IMPRIMIR num3

